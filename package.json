    "twap",
    "trading-bot"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@bluefin-exchange/bluefin-v2-client": "^6.5.1",
    "@mysten/sui": "^1.45.2",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}

